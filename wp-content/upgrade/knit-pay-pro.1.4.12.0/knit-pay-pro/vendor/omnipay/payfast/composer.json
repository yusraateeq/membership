        "merchant",
        "omnipay",
        "pay",
        "payfast",
        "payment"
    ],
    "homepage": "https://github.com/thephpleague/omnipay-payfast",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Macneil",
            "email": "adrian@adrianmacneil.com"
